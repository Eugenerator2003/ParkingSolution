@model IEnumerable<Parking.Domain.Owner>

@{
    ViewData["Title"] = "Владельцы автомобилей";
}

<h1>Владельцы автомобилей</h1>

<p>
    <a asp-action="Create">Создать запись о владельцах автомобилей</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Полное имя
            </th>
            <th>
                Номер телефона
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Fullname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PhoneNumber)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Изменить</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Подробнее</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Yдалить</a>
            </td>
        </tr>
}
    </tbody>
</table>
